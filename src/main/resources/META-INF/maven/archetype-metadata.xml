<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="quickstart-boot"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <fileSets>
    <fileSet filtered="true" encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>tsconfig.json</include>
        <include>lerna.json</include>
        <include>package.json</include>
        <include>tsconfig.base.json</include>
        <include>tslint.json</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>.gitignore.sample</include>
        <include>.gitignore</include>
      </includes>
    </fileSet>
  </fileSets>
  <modules>
    <module id="${rootArtifactId}-main" dir="__rootArtifactId__-main" name="${rootArtifactId}-main">
      <fileSets>
        <fileSet filtered="true" packaged="true" encoding="UTF-8">
          <directory>src/main/java</directory>
          <includes>
            <include>**/*.java</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/main/resources</directory>
          <includes>
            <include>**/*.xml</include>
            <include>**/*.properties</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/main/assembly</directory>
          <includes>
            <include>**/*.xml</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}-it" dir="__rootArtifactId__-it" name="${rootArtifactId}-it">
      <fileSets>
        <fileSet filtered="true" packaged="true" encoding="UTF-8">
          <directory>src/test/java</directory>
          <includes>
            <include>**/*.java</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/test</directory>
          <includes>
            <include>**/*.iml</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/test/resources</directory>
          <includes>
            <include>**/*.xml</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="${rootArtifactId}-frontend" dir="__rootArtifactId__-frontend" name="${rootArtifactId}-frontend">
      <fileSets>
        <fileSet encoding="UTF-8">
          <directory>src/overview</directory>
          <includes>
            <include>**/*.css</include>
            <include>**/*.tsx</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/time</directory>
          <includes>
            <include>**/*.tsx</include>
            <include>**/*.ts</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>public</directory>
          <includes>
            <include>**/*.html</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src</directory>
          <includes>
            <include>**/*.tsx</include>
            <include>**/*.js</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/forms</directory>
          <includes>
            <include>**/*.tsx</include>
            <include>**/*.ts</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/upload</directory>
          <includes>
            <include>**/*.tsx</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/commons</directory>
          <includes>
            <include>**/*.tsx</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/app</directory>
          <includes>
            <include>**/*.tsx</include>
            <include>**/*.ts</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory>src/words</directory>
          <includes>
            <include>**/*.tsx</include>
            <include>**/*.ts</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory></directory>
          <includes>
            <include>tsconfig.json</include>
            <include>package.json</include>
            <include>tslint.json</include>
          </includes>
        </fileSet>
        <fileSet encoding="UTF-8">
          <directory></directory>
          <includes>
            <include>dist-zip.xml</include>
            <include>.env</include>
            <include>.gitignore</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
  </modules>
</archetype-descriptor>
